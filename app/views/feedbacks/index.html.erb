<h1>Feedbacks</h1>
<table class="table text-light">
 <thead>
  <tr>
   <th>nome</th>
   <th>desc</th>
   <th>data</th>
   <th>resolvido</th>
   <th>usuario</th>
   <th colspan="3"></th>
  <tr>
 <thead>
 <tbody data-target="feedbacks.row" data-action="click->feedback#show">
   <% @feedbacks.each do |feedback| %>
    <tr>
      <td><%= feedback.nome %></td>
      <td><%= feedback.desc %></td>
      <td><%= feedback.data%></td>
      <td><%= feedback.resolvido%></td>
      <td><%= feedback.usuario%></td>
      <td><%= link_to "Mostrar",feedback%></td>
      <td><%= link_to "Editar",edit_feedback_path(feedback)%></td>
      <td><%= button_to "Deletar",feedback,method: :delete%></td>
   <% end %>
 </tbody>
</table>